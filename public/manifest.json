{
  "name": "Chrome Extension Boilerplate",
  "description": "Chrome Extension boilerplate created with React Typescript.",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
   "chrome_url_overrides": {
    "newtab": "tabs.html"
  },
   "browser_action": {
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png"
      }
    },
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "permissions": [
      "*://developer.mozilla.org/*",
      "bookmarks",
      "tabs",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": [
          "*://*/*"
        ],
        "js": [
          "content.js"
        ]
      }
    ]
  }